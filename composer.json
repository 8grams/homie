        "symfony/mailgun-mailer": "^7.2",
        "symfony/http-kernel": "^7.2",
        "symfony/http-client": "^7.2",
        "symfony/dotenv": "^7.2",
        "league/plates": "^3.6",
        "symfony/security-core": "^7.2"
    }
